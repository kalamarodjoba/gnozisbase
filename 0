	обьеденить ячейки в одну сохранив значения


=CONCATENATE(A12, CHAR(10), B12, CHAR(10), C12, CHAR(10), D12)

=CONCATENATE(A2, "-", A3, "-", A4, "-", A5, "-", A6, "-", A7)

=TEXTJOIN(",",TRUE,A2:C7)


